{
  "compilerOptions": {
    "lib": [
      "DOM",
      "ES2020"
    ],
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
}